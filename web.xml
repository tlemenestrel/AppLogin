<?xml version="1.0" encoding="UTF-8"?>
<web-app 
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0">
	<servlet>
		<servlet-name>ServletLoggers</servlet-name>
		<servlet-class>com.sdzee.servlets.ServletLoggers</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ServletUsers</servlet-name>
		<servlet-class>com.sdzee.servlets.ServletUsers</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ServletLoggers</servlet-name>
		<url-pattern>/api/v1/logs</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>ServletUsers</servlet-name>
		<url-pattern>/api/v1/users</url-pattern>
	</servlet-mapping>
	
</web-app>
